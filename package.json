{
  "name": "diademuertos",
  "version": "1.0.0",
  "engines": {
    "node": "8.9.2",
    "npm": "6.0.1",
    "yarn": "1.7.0"
  },
  "description": "Dia de Muertos Altar organizer",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "client": "cd dia-de-muertos && yarn start",
    "server": "nodemon server.js",
    "build": "cd dia-de-muertos && yarn build",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "dev:server": "cd dia-de-muertos && yarn build && cd .. && NODE_ENV=production yarn start",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd dia-de-muertos && npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emmabv1/diademuertos.git"
  },
  "author": "Emma Becerril Vilar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/emmabv1/diademuertos/issues"
  },
  "homepage": "https://github.com/emmabv1/diademuertos#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^4.0.1",
    "cookie-session": "^2.0.0-beta.3",
    "express": "4.16.3",
    "mongoose": "^5.2.13",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0"
  },
  "devDependencies": {
    "nodemon": "^1.18.4"
  }
}
